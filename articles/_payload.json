":503,"children":504},{},[505],{"type":25,"value":506},"Rice vinegar:",{"type":25,"value":118},{"type":19,"tag":38,"props":509,"children":510},{},[511,516],{"type":19,"tag":42,"props":512,"children":513},{},[514],{"type":25,"value":515},"Sesame oil:",{"type":25,"value":128},{"type":19,"tag":38,"props":518,"children":519},{},[520,525],{"type":19,"tag":42,"props":521,"children":522},{},[523],{"type":25,"value":524},"Minced garlic:",{"type":25,"value":526}," 1 clove",{"type":19,"tag":38,"props":528,"children":529},{},[530,534],{"type":19,"tag":42,"props":531,"children":532},{},[533],{"type":25,"value":136},{"type":25,"value":128},{"type":19,"tag":38,"props":536,"children":537},{},[538],{"type":19,"tag":42,"props":539,"children":540},{},[541],{"type":25,"value":542},"Sesame seeds",{"type":19,"tag":38,"props":544,"children":545},{},[546],{"type":19,"tag":42,"props":547,"children":548},{},[549],{"type":25,"value":550},"Finely chopped green onion",{"type":19,"tag":27,"props":552,"children":553},{"id":160},[554],{"type":25,"value":163},{"type":19,"tag":165,"props":556,"children":558},{"id":557},"_1-make-batter",[559],{"type":25,"value":560},"1. Make Batter",{"type":19,"tag":172,"props":562,"children":563},{},[564],{"type":25,"value":565},"In a large bowl, mix together flour, cornstarch, cold water, eggs, and salt until smooth.",{"type":19,"tag":165,"props":567,"children":569},{"id":568},"_2-prepare-vegetables",[570],{"type":25,"value":571},"2. Prepare Vegetables",{"type":19,"tag":172,"props":573,"children":574},{},[575],{"type":25,"value":576},"Wash and thinly slice or julienne the zucchini, carrots, green onions, onion, and red bell pepper. If using chili, slice it finely.",{"type":19,"tag":165,"props":578,"children":580},{"id":579},"_3-combine",[581],{"type":25,"value":582},"3. Combine",{"type":19,"tag":172,"props":584,"children":585},{},[586],{"type":25,"value":587},"Add the vegetables to the batter and mix until the vegetables are evenly coated.",{"type":19,"tag":165,"p